import java.util.Scanner;
class Main
{
public static void main(String args[])
{
Scanner sc =new Scanner(System.in);
int a,b,c,d;
double e,f;
a=sc.nextInt();
b=sc.nextInt();
c=sc.nextInt();
d=sc.nextInt();
e=(a+c)/2.0;
f=(b+d)/2.0;
System.out.println("Binoy's house is located at ("+e+","+f+")");
}
}
